{
  "name": "react-esbuild-app",
  "version": "0.0.0-0",
  "private": true,
  "keywords": [
    "template-react-esbuild"
  ],
  "scripts": {
    "build": "concurrently \"npm run build:*\"",
    "build:esbuild": "esbuild --bundle --jsx=automatic --minify --outdir=./public/static/js/ main=./src/index.tsx directline=./src/workers/directLine/index.ts embed=./src/embed/index.tsx store=./src/workers/store/index.ts",
    "build:typescript": "tsc -p ./src/tsconfig.json",
    "start": "concurrently --kill-others \"npm run start:*\"",
    "start:esbuild": "npm run build:esbuild -- --servedir=./public",
    "start:typescript": "npm run build:typescript -- --watch"
  },
  "dependencies": {
    "@emotion/css": "^11.10.6",
    "botframework-directlinejs": "^0.15.2",
    "botframework-webchat": "^4.15.7",
    "botframework-webchat-core": "^4.15.7",
    "core-js": "^3.29.1",
    "message-port-rpc": "^0.0.0-main.1ff5fd7",
    "punycode": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-wrap-with": "^0.0.2",
    "use-ref-from": "^0.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.13",
    "typescript": "^5.0.2"
  }
}
